#include<iostream>
#include<vector>
using namespace std;
int main(){
    int num;
    cin>>num;
    vector<int> arr(num);
    for(int i=0;i<num;i++){
        cin>>arr[i];
    }
    for(int i=0;i<num;i++){
        for(int j=i;j<num;j++){
            for(int k=i;k<=j;k++){
                cout<<arr[k] <<" ";
            }
            cout<<endl;
        }

    }
    return 0;
}
